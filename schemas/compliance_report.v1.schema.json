{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://superhyperion.dev/schemas/compliance_report.v1.schema.json",
  "title": "TRUST-1.0.3 Compliance Report v1",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "contract_version",
    "tenant_id",
    "total_runs",
    "counts",
    "rates",
    "hold_codes",
    "replay",
    "latency",
    "runs"
  ],
  "properties": {
    "contract_version": {"const": "v1"},
    "tenant_id": {"type": ["string", "null"]},
    "total_runs": {"type": "integer", "minimum": 0},
    "counts": {"type": "object"},
    "rates": {"type": "object"},
    "hold_codes": {"type": "array"},
    "replay": {"type": "object"},
    "latency": {"type": "object"},
    "runs": {"type": "array"}
  }
}
